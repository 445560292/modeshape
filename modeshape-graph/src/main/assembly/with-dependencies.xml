<?xml version="1.0"?>
<assembly>
	<id>jar-with-dependencies</id>
	<includeBaseDirectory>false</includeBaseDirectory>
	<formats>
		<format>jar</format>
	</formats>
	<dependencySets>
		<dependencySet>
			<unpack>true</unpack>
			<scope>runtime</scope>
            <unpackOptions>
				<excludes>
				<exclude>/META-INF/*.RSA</exclude>
				<exclude>/META-INF/*.DSA</exclude>
				<exclude>/META-INF/*.SF</exclude>
				<exclude>/META-INF/*.rsa</exclude>
				<exclude>/META-INF/*.dsa</exclude>
				<exclude>/META-INF/*.sf</exclude>
				</excludes>
            </unpackOptions>
		</dependencySet>
	</dependencySets>
