{
    "name": "Persistent Repository With Legacy FS Binary Store",
    "storage": {
        "persistence" : {
            "type" : "db",
            "connectionUrl": "jdbc:h2:file:./target/persistent_repository/db"
        },
        "binaryStorage": {
            "type": "file",
            "directory": "target/legacy_fs_binarystore"
        }
    },
    "workspaces": {
        "default": "default",
        "allowCreation": true
    }
}