{
    "name" : "Federated repository",
    "workspaces" : {
        "default" : "default",
        "allowCreation" : true
    },
    "externalSources" : {
        "targetDirectory" : {
            "classname" : "filesystem",
            "directoryPath" : "target/classes",
            "extraPropertiesStorage" : "json",
            "projections" : [
                "default:/fs => /"
            ]
        }
    },
    "query" : {
        "indexing" : {
            "rebuildOnStartup" : {
                "when" : "never"
            }
        }
    }
}