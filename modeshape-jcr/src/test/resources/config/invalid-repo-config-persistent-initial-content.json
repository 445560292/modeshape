{
    "name" : "Persistent Repository With Initial Content",
    "storage" : {
        "persistence" : {
            "type" : "db",
            "connectionUrl": "jdbc:h2:file:./target/persistent_repository/db"
        }    
    },
    "workspaces" : {
        "predefined" : ["ws1"],
        "default" : "default",
        "allowCreation" : true,
        "initialContent" : {
            "ws1" : "xmlImport/docWithInvalidCharacters.xml"
        }
    },
    "security" : {
        "anonymous" : {
            "roles" : ["readonly","readwrite","admin"],
            "useOnFailedLogin" : false
        }
    }
}