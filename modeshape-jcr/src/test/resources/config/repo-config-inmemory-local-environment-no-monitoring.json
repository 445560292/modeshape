{
    "name" : "No Monitoring Repository",
    "monitoring" : {
        "enabled" : false
    },
    "storage" : {
        "persistence" : {
            "type" : "db",
            "connectionUrl": "jdbc:h2:file:./target/persistent_repository/db"
        }
    },
    "workspaces" : {
        "default" : "default",
        "allowCreation" : true
    },
    "security" : {
        "anonymous" : {
            "roles" : ["readonly","readwrite","admin"],
            "useOnFailedLogin" : false
        }
    }
}