{
    "name" : "Demo Repository",
    "workspaces" : {
        "default" : "default",
        "allowCreation" : true
    },
    "storage" : {
        "persistence" : {
            "type" : "db",
            "connectionUrl" : "jdbc:h2:mem:test;DB_CLOSE_DELAY=0",
            "dropOnExit" : true
        },
    },
    "security" : {
        "anonymous" : {
            "roles" : ["readonly","readwrite","admin"],
            "useOnFailedLogin" : false
        }
    }
}